                        </Grid.ColumnDefinitions>
                        <Button Content="New Account" Grid.Row="1" Grid.Column="0"  Style="{DynamicResource OptionsButton}" Click="CreateAccountOption_Click"></Button>
                        <Button Content="Import" Grid.Row="1" Grid.Column="1"  Style="{DynamicResource OptionsButton}"></Button>
                        <Button Content="Delete All" Grid.Row="1" Grid.Column="5"  Style="{DynamicResource OptionsButton}" Click="DeleteAllAccounts_Click"></Button>
                    </Grid>
                    <Grid Grid.Row="1" x:Name="AccountPageList">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="*"></RowDefinition>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <ListView x:Name="AccountsListView" Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="8" SizeChanged="AccountsListViewSizeChanged">
                            <ListView.View>
                                <GridView>
                                    <GridViewColumn Header="#" DisplayMemberBinding="{Binding AccountId}" />
                                    <GridViewColumn Header="store" DisplayMemberBinding="{Binding Store}" />
                                    <GridViewColumn Header="e-mail" DisplayMemberBinding="{Binding Email}" />
                                    <GridViewColumn Header="password" DisplayMemberBinding="{Binding Password}" />
                                    <GridViewColumn Header="Actions">
                                        <GridViewColumn.CellTemplate>
                                            <DataTemplate>
                                                <StackPanel Orientation="Horizontal">
                                                    <Button Content="Edit" CommandParameter="{Binding}" Style="{DynamicResource ListOptionsButton}" Click="EditAccount_Click" />
                                                    <Button Content="Delete" CommandParameter="{Binding}" Style="{DynamicResource ListOptionsButton}" Click="DeleteSpecificAccount_Click" />
                                                </StackPanel>
                                            </DataTemplate>
                                        </GridViewColumn.CellTemplate>
                                    </GridViewColumn>
                                </GridView>
                            </ListView.View>
                        </ListView>
                    </Grid>

                    <!--#region Create Account-->
                    <Grid Grid.Row="1" x:Name="CreateAccountWindow" Visibility="Hidden">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="0.20*" />
                            <RowDefinition />
                            <RowDefinition Height="0.40*" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="0.15*" />
                            <ColumnDefinition />
                            <ColumnDefinition Width="0.15*" />
                        </Grid.ColumnDefinitions>
                        <Grid Grid.Row="1" Grid.Column="1">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="10px" />
                                <RowDefinition Height="35px" />
                                <RowDefinition Height="25px" />
                                <RowDefinition Height="40px" />
                                <RowDefinition Height="15px" />
                                <RowDefinition Height="25px" />
                                <RowDefinition Height="40px" />
                                <RowDefinition Height="15px" />
                                <RowDefinition Height="25px" />
                                <RowDefinition Height="40px" />
                                <RowDefinition Height="35px" />
                                <RowDefinition Height="*" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="20px" />
                                <ColumnDefinition />
                                <ColumnDefinition />
                                <ColumnDefinition Width="25px" />
                                <ColumnDefinition Width="20px" />
                            </Grid.ColumnDefinitions>
                            <Grid Grid.Row="10" Grid.Column="2">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition />
                                    <ColumnDefinition />
                                </Grid.ColumnDefinitions>
                                <Button x:Name="CreateAccountButton" Grid.Column="1" Style="{DynamicResource RoundedButtonStyle}" Background="ForestGreen" Foreground="Wheat" FontWeight="Bold" FontSize="15px" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Content="Create" Margin="50,0,0,0" Click="CreateAccount_Click" />
                                <Button x:Name="SaveEditedAccountButton" Visibility="Hidden" Grid.Column="1" Style="{DynamicResource RoundedButtonStyle}" Background="ForestGreen" Foreground="Wheat" FontWeight="Bold" FontSize="15px" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Content="Save" Margin="50,0,0,0" Click="SaveEditedAccount_CLick" />
                            </Grid>
                            <Label x:Name="CreateAccountLabel" Grid.Row="1" Grid.Column="1" Content="Create Account" VerticalAlignment="Center" FontWeight="UltraBlack" FontSize="20px" Foreground="FloralWhite" />
                            <Button x:Name="CancelCreateAccont" Style="{DynamicResource ApplicationButtonStyle}" Grid.Column="3" Grid.Row="1" HorizontalAlignment="Right" Height="20px" Width="20px" VerticalAlignment="Center" BorderThickness="0" Background="#09161f" Content="✖" Foreground="DeepSkyBlue" FontSize="13" FontFamily="Fonts/Dosis" FontWeight="Medium" Margin="0,0,2.5,0" Click="CancleCreateAccount_Click"></Button>

                            <Label Grid.Row="2" Grid.Column="1" Content="Store" VerticalAlignment="Center" FontWeight="DemiBold" FontSize="13px" Foreground="LightSlateGray" />
                            <ComboBox x:Name="newAccount_Store" Grid.Row="3" Grid.Column="1" IsEditable="False" Style="{DynamicResource theComboBox}" ItemContainerStyle="{DynamicResource theComboBoxItem}">
                                <ComboBoxItem Content="KITH" />
                            </ComboBox>

                            <Label Grid.Row="5" Grid.Column="1" Content="E-Mail" VerticalAlignment="Center" FontWeight="DemiBold" FontSize="13px" Foreground="LightSlateGray"></Label>
                            <TextBox x:Name="newAccount_Email" Grid.Column="1" Grid.ColumnSpan="2" Grid.Row="6"></TextBox>

                            <Label Grid.Row="8" Grid.Column="1" Content="Password" VerticalAlignment="Center" FontWeight="DemiBold" FontSize="13px" Foreground="LightSlateGray" />
                            <TextBox x:Name="newAccount_Password" Grid.Row="9" Grid.Column="1"></TextBox>

                            <Label x:Name="newAccount_errorStore" HorizontalContentAlignment="Center" Visibility="Hidden" Grid.Row="4" Grid.Column="1" Content="Please choose a store" VerticalContentAlignment="Center" Foreground="#D32A0E" FontSize="12px" Padding="4,0,0,0" />
                            <Label x:Name="newAccount_errorEmail" HorizontalContentAlignment="Center" Visibility="Hidden" Grid.Row="7" Grid.Column="1" Content="Please enter an email" VerticalContentAlignment="Center" Foreground="#D32A0E" FontSize="12px" Padding="4,0,0,0" />
                            <Label x:Name="newAccount_errorPassword" HorizontalContentAlignment="Center" Visibility="Hidden" Grid.Row="10" Grid.Column="1" Content="Please enter a Password" VerticalContentAlignment="Center" Foreground="#D32A0E" FontSize="12px" Padding="4,0,0,0" />
                            <Grid.Background>
                                <SolidColorBrush Color="#081D26" />
                            </Grid.Background>
                        </Grid>
                    </Grid>
                    <!--#endregion-->
                </Grid>
                <!--#endregion-->

                <!--#region Settings Page-->
                <Grid x:Name="SettingsPage" Visibility="Hidden" Grid.Row="1">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition></ColumnDefinition>
                        <ColumnDefinition Width="5*"></ColumnDefinition>
                        <ColumnDefinition></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="0.15*"></RowDefinition>
                        <RowDefinition Height="0.3*"></RowDefinition>
                        <RowDefinition></RowDefinition>
                    </Grid.RowDefinitions>
                    <Border CornerRadius="10" BorderThickness="8" BorderBrush="#09161f" Margin="6,2,6,2" Grid.Row="1" Grid.Column="1">
                        <Grid x:Name="UserInfoGrid" Grid.Row="0" Background="#09161f">
                            <Grid.RowDefinitions>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="0.1*"></ColumnDefinition>
                                <ColumnDefinition Width="0.4*"></ColumnDefinition>
                                <ColumnDefinition Width="0.1*"></ColumnDefinition>
                                <ColumnDefinition></ColumnDefinition>
                                <ColumnDefinition Width="0.3*"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Image Grid.RowSpan="2" Grid.Column="1" Source="Images/usericon.png" ></Image>
                            <Label Grid.Row="2" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="DeepSkyBlue" FontSize="15" FontFamily="Fonts/Dosis" FontWeight="Medium"></Label>
                        </Grid>
                    </Border>
                </Grid>
                <!--#endregion-->

                <!--#region Background-->
                <Grid.Background>
                    <LinearGradientBrush>
                        <GradientStop Color="#245A46" Offset="-0.2"></GradientStop>
                        <GradientStop Color="#243F51" Offset="0.2"></GradientStop>
                        <GradientStop Color="#3E2D7C" Offset="1.9"></GradientStop>
                    </LinearGradientBrush>
                </Grid.Background>
                <!--#endregion-->
            </Grid>

            <!--#region Nav Grid-->
            <Grid x:Name="NavGrid" Grid.Column="0" Background="#0e2433">
                <Grid.RowDefinitions>
                    <RowDefinition x:Name="LogoUserName" Height="100"></RowDefinition>
                    <RowDefinition Height="*"></RowDefinition>
                    <RowDefinition Height="Auto"></RowDefinition>
                    <RowDefinition Height="Auto"></RowDefinition>
                    <RowDefinition Height="Auto"></RowDefinition>
                    <RowDefinition Height="Auto"></RowDefinition>
                    <RowDefinition Height="*"></RowDefinition>
                    <RowDefinition Height="Auto"></RowDefinition>
                </Grid.RowDefinitions>
                <Border CornerRadius="10" BorderThickness="8" BorderBrush="#09161f" Margin="6,2,6,2">
                    <Grid x:Name="LogoUserNameGrid" Grid.Row="0" Background="#09161f">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="50px" />
                            <RowDefinition Height="50px" />
                        </Grid.RowDefinitions>
                        <Image Grid.Row="0" Source="/Images/sneaker.png"></Image>
                        <Label x:Name="UserNameLable" Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="DeepSkyBlue" FontSize="12" FontFamily="Fonts/Dosis" FontWeight="Medium" >SMResell</Label>
                    </Grid>
                </Border>
                <Grid Grid.Row="1" Margin="0,2,0,0">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="30px" />
                        <RowDefinition Height="30px" />
                        <RowDefinition />
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="15px"></ColumnDefinition>
                        <ColumnDefinition Width="*"></ColumnDefinition>
                        <ColumnDefinition Width="*"></ColumnDefinition>
                        <ColumnDefinition Width="*"></ColumnDefinition>
                        <ColumnDefinition Width="*"></ColumnDefinition>
                        <ColumnDefinition Width="10px"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <!--Tasks-->
                    <Border Background="#09161f" Grid.Column="1" Grid.Row="1" Width="35" HorizontalAlignment="Left" VerticalAlignment="Center" CornerRadius="0,0,8,8">
                        <Label x:Name="RunningTaskLabel" Content="0" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="15" Foreground="White" Height="30" Padding="0,0,0,5" />
                    </Border>
                    <Border Background="#09161f" Grid.Column="1" Grid.Row="0" Width="35" HorizontalAlignment="Left" VerticalAlignment="Center" CornerRadius="8,8,0,0">
                        <Label Content="↻" Padding="0,3,0,0" HorizontalContentAlignment="Center" FontSize="18" Foreground="White" Height="30" />
                    </Border>
                    <!--Cart-->
                    <Border Background="#09161f" Grid.Column="2" Grid.Row="1" Width="35" HorizontalAlignment="Left" VerticalAlignment="Center" CornerRadius="0,0,8,8">
                        <Label Content="0" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="15" Foreground="Orange" Height="30" Padding="0,0,0,5" />
                    </Border>
                    <Border Background="#09161f" Grid.Column="2" Grid.Row="0" Width="35" HorizontalAlignment="Left" VerticalAlignment="Center" CornerRadius="8,8,0,0">
                        <Label Content="🛒" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="12" Foreground="Orange" Height="30" />
                    </Border>
                    <!--Checkout-->
                    <Border Background="#09161f" Grid.Column="3" Grid.Row="1" Width="35" HorizontalAlignment="Left" VerticalAlignment="Center" CornerRadius="0,0,8,8">
                        <Label x:Name="CheckoutCounter" Content="0" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="15" Foreground="Green" Height="30" Padding="0,0,0,5" />
                    </Border>
                    <Border Background="#09161f" Grid.Column="3" Grid.Row="0" Width="35" HorizontalAlignment="Left" VerticalAlignment="Center" CornerRadius="8,8,0,0">
                        <Label Content="✓" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="15" Foreground="Green" />
                    </Border>
                    <!--Failures-->
                    <Border Background="#09161f" Grid.Column="4" Grid.Row="1" Width="35" HorizontalAlignment="Left" VerticalAlignment="Center" CornerRadius="0,0,8,8">
                        <Label Content="0" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="15" Height="30" Foreground="Red" Padding="0,0,0,5" />
                    </Border>
                    <Border Background="#09161f" Grid.Column="4" Grid.Row="0" Width="35" HorizontalAlignment="Left" VerticalAlignment="Center" CornerRadius="8,8,0,0">
                        <Label Content="✖" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="12" Foreground="Red" Height="30" />
                    </Border>
                </Grid>
                <Grid x:Name="TasksGrid" Grid.Row="2">
                    <Button x:Name="Tasks" BorderThickness="0" MinHeight="35" Background="#09161f" Content="Tasks" Foreground="DeepSkyBlue" FontSize="15" FontFamily="Fonts/Dosis" FontWeight="Medium" Style="{DynamicResource RoundedButtonStyle}" Click="Page_Click" ></Button>
                </Grid>
                <Grid x:Name="ProxiesGrid" Grid.Row="3">
                    <Button x:Name="Proxies" BorderThickness="0" MinHeight="35" Background="#09161f" Content="Proxies" Foreground="DeepSkyBlue" FontSize="15" FontFamily="Fonts/Dosis" FontWeight="Medium" Style="{DynamicResource RoundedButtonStyle}" Click="Page_Click" ></Button>
                </Grid>
                <Grid x:Name="ProfilesGrid" Grid.Row="4">
                    <Button x:Name="Profiles" BorderThickness="0" MinHeight="35" Background="#09161f" Content="Profiles" Foreground="DeepSkyBlue" FontSize="15" FontFamily="Fonts/Dosis" FontWeight="Medium" Style="{DynamicResource RoundedButtonStyle}" Click="Page_Click" ></Button>
                </Grid>
                <Grid x:Name="AccountsGrid" Grid.Row="5">
                    <Button x:Name="Accounts" BorderThickness="0" MinHeight="35" Background="#09161f" Content="Accounts" Foreground="DeepSkyBlue" FontSize="15" FontFamily="Fonts/Dosis" FontWeight="Medium" Style="{DynamicResource RoundedButtonStyle}" Click="Page_Click" ></Button>
                </Grid>
                <Grid x:Name="SettingsGrid" Grid.Row="7">
                    <Button x:Name="Settings" BorderThickness="0" MinHeight="35" Background="#09161f" Content="Settings" Foreground="DeepSkyBlue" FontSize="15" FontFamily="Fonts/Dosis" FontWeight="Medium" Style="{DynamicResource RoundedButtonStyle}" Click="Page_Click" ></Button>
                </Grid>
            </Grid>
            <!--#endregion-->
        </Grid>
    </Border>
</Window>